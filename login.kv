<calibrateRightHandFlexWindow>:
	FloatLayout:
	    canvas:
            Color:
                rgba: 0.5, 0.5, 0.5, 0.5
            Rectangle: #woohoo!!!
                size: self.size
                pos: self.pos
		        size: root.width, root.height
		Label:
			text : " INSTRUCTIONS "
			size_hint : 0.3, 0.1
			pos_hint : {"x":0.35, "top":1}
		Label:
			text : " Flexing fingers "
			size_hint : 0.5, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.9}
		Image:
            source: '/Users/chuks/Downloads/webmd_rf_photo_of_fist_stretch.jpg'
            pos_hint : {"x":0.35, "top":0.7}
            size_hint : 0.3,0.3
            allow_stretch: True
        Label:
			text : "1. Release and spread your fingers wide. "
			size_hint : 0.8, 0.1
			pos_hint : {"x":0.1, "top":0.4}
		Label:
			text : "2. Make a gentle fist, wrapping your thumb across your fingers, "
			size_hint : 0.8, 0.1
			pos_hint : {"x":0.1, "top":0.3}
		Label:
			text : "3. Repeat and Hold for 30 to 60 seconds "
			size_hint : 0.8, 0.1
			pos_hint : {"x":0.1, "top":0.2}
        Button:
            text: "START"
            size_hint : 0.3, 0.1
			pos_hint : {"x":0.7, "top":0.1}
			on_release:
			    root.calibrate()
		Button:
            text: "BACK"
            size_hint : 0.3, 0.1
			pos_hint : {"x":0, "top":0.1}
			on_release:
			    root.back()

