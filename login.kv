# there are three screens
windowManager:
	loginWindow:
	signupWindow:
	logDataWindow:
	calibrateLeftHandWindow:
	calibrateRightHandWindow:
	calibrateLeftLegWindow:
	calibrateRightLegWindow:
	calibrateWindow:

# GUI for the login window
<loginWindow>:
	username : username
	password : password
	FloatLayout:
		Label:
			text : "USERNAME: "
			size_hint : 0.2, 0.1
			pos_hint : {"x":0.25, "top":0.9}
		TextInput:
			id : username
			multiline :False
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.45, "top" : 0.9}
		Label:
			text : "PASSWORD: "
			size_hint : 0.2, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.7}
		TextInput:
			id : password
			multiline :False
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.45, "top" : 0.7}
		Button:
			text : "Create an account"
			size_hint : 0.4, 0.1
			pos_hint : {"x" : 0.33, "top" : 0.4}
			on_release:
				app.root.current = 'signup'
				root.manager.transition.direction = "left"
		Button:
			text : "LOGIN"
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.39, "top" : 0.2}
			on_release:
				root.validate()
				root.manager.transition.direction = "up"



# GUI for the signup window
<signupWindow>:
	username : username
	password : password
	FloatLayout:
		Label:
			text : "NAME: "
			size_hint : 0.2, 0.1
			pos_hint : {"x":0.25, "top":0.9}
		TextInput:
			id : username
			multiline : False
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.45, "top" : 0.9}
		Label:
			text : "PASSWORD: "
			size_hint : 0.2, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.7}
		TextInput:
			id : password
			multiline : False
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.45, "top" : 0.7}
		Button:
			text : " SUBMIT"
			size_hint : 0.3, 0.1
			pos_hint : {"x" : 0.39, "top" : 0.28}
			on_press :
				root.signupbtn()
				root.manager.transition.direction = "right"


<calibrateWindow>:
	FloatLayout:
		Button:
			text : "Calibrate Left Hand"
			size_hint : 0.5, 0.1
			pos_hint : {"x":0.25, "top":0.9}
			on_release:
			    app.root.current='calibratelefthandwindow'
			    root.manager.transition.direction = "left"
		Button:
			text : "Calibrate Right Hand"
			size_hint : 0.5, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.7}
			on_release:
			    app.root.current='calibraterighthandwindow'
			    root.manager.transition.direction = "up"

		Button:
			text : "Calibrate Left Leg"
			size_hint : 0.5, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.5}
			on_release:
			    app.root.current='calibrateleftlegwindow'
			    root.manager.transition.direction = "right"
		Button:
			text : "Calibrate Right Leg"
			size_hint : 0.5, 0.1
			pos_hint : {"x" : 0.25, "top" : 0.3}
			on_release:
			    app.root.current='calibraterightlegwindow'
			    root.manager.transition.direction = "left"
		Button:
		    text: "Back"
		    size_hint : 0.2,0.1
		    pos_hint : {"x" : 0.1, "top" : 0.1}
		    on_release:
		        app.root.current= 'logdata'
		        root.manager.transition.direction = "right"
		Button:
		    text: "Get ML Prediction"
		    size_hint : 0.2,0.1
		    pos_hint : {"x" : 0.7, "top" : 0.1}
		    on_release:
		        root.getMlPrediction()
		        root.manager.transition.direction = "right"

